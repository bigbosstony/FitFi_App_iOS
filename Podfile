target 'FitfiApp' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
	
	pod 'SwiftCharts', '~> 0.6.1'
	
	 pod 'JTAppleCalendar', '~> 7.0'

    pod 'Alamofire', '~> 4.7'
	pod 'Charts'

  # Pods for FitfiApp


  target 'FitfiAppTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FitfiAppUITests' do
    inherit! :search_paths
    # Pods for testing
  end



end
